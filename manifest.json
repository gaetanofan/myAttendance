{
  "name": "Partecipanti Google Meet",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "Memorizza su file i partecipanti del meeting con un solo click!",
  "homepage_url": "https://github.com/jakecrowley/Google-Meet-Attendance",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "it",
  "background": {
    "scripts": ["js/inject.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Instructions Popup",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
	"tabs",
    "contextMenus",
    "https://meet.google.com/*"
  ]
}
